<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            prefHeight="240.0" prefWidth="450.0" styleClass="anchor-pane">
    <stylesheets>
        <URL value="@css/download.css"/>
    </stylesheets>

    <ProgressBar AnchorPane.rightAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.bottomAnchor="50" progress="0.0" id="progress"/>
    <Button text="Отмена" AnchorPane.bottomAnchor="10" AnchorPane.rightAnchor="10" prefWidth="120" id="cancelButton"/>
    <Label text="Хеширование" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" id="caption"/>
    <Label text="..." AnchorPane.leftAnchor="10" AnchorPane.topAnchor="50" id="info"/>

</AnchorPane>
